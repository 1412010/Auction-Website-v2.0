{{> categoriesPanel this}}
<!-- Slider -->
<div class="col-md-9">
    <div id="myCarousel" class="carousel slide" data-ride="carousel">
        <!-- Indicators -->
        <ol class="carousel-indicators">
            <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
            <li data-target="#myCarousel" data-slide-to="1"></li>
            <li data-target="#myCarousel" data-slide-to="2"></li>
        </ol>
        <!-- Wrapper for slides -->
        <div class="carousel-inner">
            <div class="item active">
                <img src="/Imgs/adver/c1.png" alt="c1">
            </div>
            <div class="item">
                <img src="/Imgs/adver/c2.png" alt="c2">
            </div>
            <div class="item">
                <img src="/Imgs/adver/c3.png" alt="c3">
            </div>
        </div>
        <!-- Left and right controls -->
        <a class="left carousel-control" href="#myCarousel" data-slide="prev">
            <span class="glyphicon glyphicon-chevron-left"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="right carousel-control" href="#myCarousel" data-slide="next">
            <span class="glyphicon glyphicon-chevron-right"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
    <div class="row">
        &nbsp
    </div>
    <!-- Top ra giá -->
    <div class="panel panel-default" style="border-color: #189eff; ">
        <div class="panel-heading" style=" background: #189eff;">
            <span class="panel-title">Top ra giá</span>
        </div>
        <div class="panel-body">
            <div class="row">
                <div class="span12">
                    <div class="well" style="padding-bottom: 0;">
                        <div id="myCarousel1" class="carousel slide">
                            <!-- Carousel items -->
                            <div class="carousel-inner">
                                <div class="item active">
                                    <div class="row-fluid">
                                        {{#each topRaGia1}}
                                            <div class="col-md-4">
                                                <div class="thumbnail" style="border-color: #189eff;">
                                                    <a href="/product/detail/{{madaugia}}"><img src="/Imgs/sp/{{madaugia}}/main_thumbs.jpg" title="{{tensp}}" alt="{{tensp}}" style="max-width:100%;" /></a>
                                                    <div class="caption" style=" background: #eee;">
                                                        <h5 style="margin: 0 0; font-weight: bold;">{{tensp}}</h5>
                                                        <div>
                                                            <span>Giá hiện tại: </span> {{formatNumber giahientai}}
                                                            <br/>
                                                            <span>Lượt đầu giá: </span> {{solandaugia}}
                                                            <br/>
                                                            <span>Thời gian còn lại:</span> {{timeRemain tgketthuc}}
